secondary\"         \n                        >\n                            {\n                                subcategorias.map(item => <MenuItem key={item} value={item}>{item}</MenuItem>)\n                            }\n                        </Select>                       \n                    </Grid>\n                    <Grid item xs={12} md={3}>                        \n                        <InputLabel id=\"producto\">Producto</InputLabel>\n                        <Select\n                            required\n                            id=\"producto\"\n                            name='producto'\n                            value={producto}\n                            onChange={handleChange}\n                            fullWidth\n                            color=\"secondary\"         \n                        >\n                            {\n                                productos.map(item => <MenuItem key={item} value={item}>{item}</MenuItem>)\n                            }                                    \n                        </Select>                       \n                    </Grid>\n                    <Grid item xs={12} md={3}>\n                        <TextField                                    \n                            id=\"unidad\"\n                            name=\"unidad\"\n                            label=\"Unidad\"\n                            disabled\n                            value={unidad}\n                            onChange={handleChange}\n                            fullWidth  \n                            color=\"secondary\"                                           \n                        />\n                    </Grid>\n                </Grid>\n                <Grid container spacing={3}>\n                    <Grid item xs={12} md={3}>\n                        <TextField\n                            disabled\n                            id=\"requeridos\"\n                            name=\"requeridos\"